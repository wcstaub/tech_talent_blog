<!DOCTYPE html>
<html>
  <head>
    <title>TechTalentBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<!-- Added this container div to use bootstrap per TTS slides -->
 	<div class="container">
  		<div class="pull-right">
   			 <% if user_signed_in? %>
     			 <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-danger" %>
    		<% end %>
  		</div>
  		<!-- yeild renders the template of the current controller/action-->
    	<%= yield %>
	</div>
  </body>
</html>
